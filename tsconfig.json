{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules",
        "typings",
        "front",
        "functions/node_modules/@types"
    ],
    "filesGlob": [
        "**/*.ts",
        "node_modules/@types",
        "functions/node_modules/@types"
    ]
}